{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/identityapp-.log",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "rollingInterval": "Month"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId",
      "WithExceptionDetails"
    ]
  },
  "ConnectionStrings": {
    "LocalMainDatabase": "Server=localhost; Database=main_logistics; Uid=root; Pwd=Test123456; Connect Timeout=10"
  },
  "GoogleRecaptcha": {
    "SiteKey": "6LdHP4ggAAAAAFWk718EvMcWQ5CI21iwXp0UhO8q"
  },
  "EmailConfig": {
    "SenderMail": "noreply@suxrobgm.net",
    "SenderName": "Logistics NoReply",
    "UserName": "noreply@suxrobgm.net",
    "Host": "mail.hosting.reg.ru",
    "Port": 587
  },
  "TenantsConfig": {
    "DatabaseProvider": "MySql",
    "DatabaseHost": "localhost",
    "DatabaseUserId": "root",
    "DatabasePassword": "Test123456"
  }
}